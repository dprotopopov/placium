{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:60353",
      "sslPort": 44391
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "mysql",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "MySqlQueryTools": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "mysql",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:9433;http://localhost:9090"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/mysql",
      "publishAllPorts": true,
      "useSSL": true,
      "DockerfileRunArguments": "-e \"ConnectionStrings__SphinxConnection\"=\"Host=host.docker.internal;Port=9306;Command Timeout=0\""
    }
  }
}